<script>
    // Function to check if the user is on a mobile device
    function isMobileDevice() {
        return window.innerWidth <= 767; // Adjust breakpoint as needed
    }

    // Function to show the popup after 15 seconds on mobile devices
    function showPopupAfterDelay() {
        if (isMobileDevice()) {
            setTimeout(function () {
                var popup = document.getElementById("boxes");
                if (popup) {
                    popup.style.display = "block";
                }
            }, 10000); // 10 seconds delay
        }
    }

    // Call the function on page load
    document.addEventListener("DOMContentLoaded", showPopupAfterDelay);
</script>

